package Patrones;

/**
 *
 * @author user
 */
public class PlantillaNotificadorCambioNota extends ObservadorNotificador {
    @Override
    protected String generarEncabezado() {
        return "[Notificador] --- Cambio de Nota ---";
    }

    @Override
    protected String generarCuerpo(String mensaje) {
        return "Detalle del cambio: " + mensaje;
    }

    @Override
    protected String generarPie() {
        return "[Notificador] Fin del mensaje.";
    }
}
