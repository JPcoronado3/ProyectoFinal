package Patrones;

/**
 *
 * @author user
 */

public class PlantillaBitacoraCambioNota extends ObservadorNotificador {
    @Override
    protected String generarEncabezado() {
        return "[Bitácora] --- Registro de Actividad ---";
    }

    @Override
    protected String generarCuerpo(String mensaje) {
        return "Registro: " + mensaje;
    }

    @Override
    protected String generarPie() {
        return "[Bitácora] Registro finalizado.";
    }
}
